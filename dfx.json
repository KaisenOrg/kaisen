{
  "version": 1,
  "canisters": {
    "internet_identity": {
      "type": "custom",
      "candid": "https://github.com/dfinity/internet-identity/releases/latest/download/internet_identity.did",
      "wasm": "https://github.com/dfinity/internet-identity/releases/latest/download/internet_identity_dev.wasm.gz",
      "remote": {
        "id": {
          "ic": "rdmx6-jaaaa-aaaaa-aaadq-cai"
        }
      },
      "frontend": {},
      "declarations": {
        "output": "src/declarations/internet_identity"
      }
    },
    "kai_backend": {
      "type": "motoko",
      "main": "backend/kai/main.mo",
      "dependencies": [
        "tracks_backend",
        "chats_backend"
      ],
      "declarations": {
        "output": "src/declarations/kai_backend"
      }
    },
    "icrc1_ledger": {
      "type": "custom",
      "wasm": "backend/icrc1_ledger.wasm.gz",
      "candid": "backend/icrc1_ledger.did",
      "declarations": {
        "output": "src/declarations/icrc1_ledger"
      }
    },
    "chats_backend": {
      "type": "motoko",
      "main": "backend/chats/main.mo",
      "declarations": {
        "output": "src/declarations/chats_backend"
      }
    },
    "tracks_backend": {
      "type": "motoko",
      "main": "backend/tracks/main.mo",
      "declarations": {
        "output": "src/declarations/tracks_backend"
      }
    },
    "nft_certificates": {
      "type": "motoko",
      "main": "backend/nft/main.mo",
      "declarations": {
        "output": "src/declarations/nft_certificates"
      }
    },
    "users_backend": {
      "type": "motoko",
      "main": "backend/users/main.mo",
      "declarations": {
        "output": "src/declarations/users_backend"
      }
    },
    "kaizen_frontend": {
      "type": "assets",
      "source": ["out"],
      "dependencies": [
        "kai_backend",
        "tracks_backend",
        "chats_backend",
        "users_backend",
        "icrc1_ledger",
        "nft_certificates"
      ],
      "declarations": {
        "output": "src/declarations/kaizen_frontend"
      }
    }
  },
  "output_env_file": ".env",
  "defaults": {
    "build": {
      "packtool": "",
      "args": ""
    }
  }
}